<?xml version='1.0' encoding='UTF-8'?>
<MorpheusModel version="4">
    <Description>
        <Details></Details>
        <Title>Virus transmission</Title>
    </Description>
    <Space>
        <Lattice class="square">
            <Neighborhood>
                <Order>1</Order>
            </Neighborhood>
            <Size value="45, 45,   0.0" symbol="size"/>
        </Lattice>
        <SpaceSymbol symbol="space"/>
    </Space>
    <Time>
        <StartTime value="0"/>
        <StopTime value="50"/>
        <TimeSymbol symbol="time"/>
    </Time>
    <Analysis>
        <ModelGraph format="dot" reduced="false" include-tags="#untagged"/>
        <Gnuplotter decorate="true" time-step="1.0">
            <Plot>
                <Cells value="cell.type">
                    <ColorMap>
                        <Color color="grey80" value="0"/>
                        <Color color="green" value="1"/>
                    </ColorMap>
                </Cells>
                <CellLabels value="V"/>
            </Plot>
            <Terminal name="png"/>
        </Gnuplotter>
        <Gnuplotter time-step="1.0">
            <Plot>
                <Field symbol-ref="Ve">
                    <ColorMap>
                        <Color color="light-blue" value="0"/>
                        <Color color="light-green" value="0.5"/>
                    </ColorMap>
                </Field>
            </Plot>
            <Terminal name="png"/>
        </Gnuplotter>
        <!--    <Disabled>
        <Gnuplotter time-step="1.0">
            <Plot>
                <Cells value="V_c2c">
                    <ColorMap>
                        <Color color="light-blue" value="0"/>
                        <Color color="light-green" value="1"/>
                    </ColorMap>
                </Cells>
            </Plot>
            <Terminal name="png"/>
        </Gnuplotter>
    </Disabled>
-->
        <Logger time-step="1.0">
            <Input>
                <Symbol symbol-ref="V"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <Logger name="populations" time-step="1.0">
            <Input>
                <Symbol symbol-ref="celltype.target.size"/>
                <Symbol symbol-ref="celltype.infected.size"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <Logger time-step="1.0">
            <Input>
                <Symbol symbol-ref="cell.id"/>
                <Symbol symbol-ref="cell.type"/>
                <Symbol symbol-ref="cell.center.x"/>
                <Symbol symbol-ref="cell.center.y"/>
                <Symbol symbol-ref="Vi"/>
                <!--    <Disabled>
        <Symbol symbol-ref="V_c2c"/>
    </Disabled>
-->
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <ContactLogger time-step="1.0" ignore-medium="true"/>
        <Logger time-step="1.0">
            <Input>
                <Symbol symbol-ref="space.x"/>
                <Symbol symbol-ref="space.y"/>
                <Symbol symbol-ref="cell.id"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <Logger time-step="1000">
            <Input>
                <!--    <Disabled>
        <Symbol symbol-ref="b_c2c"/>
    </Disabled>
-->
                <Symbol symbol-ref="b_cf"/>
                <Symbol symbol-ref="c_V"/>
                <Symbol symbol-ref="D_V"/>
                <!--    <Disabled>
        <Symbol symbol-ref="l_c2c"/>
    </Disabled>
-->
                <Symbol symbol-ref="p_V"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
        <Logger time-step="1.0">
            <Input>
                <Symbol symbol-ref="Ve"/>
            </Input>
            <Output>
                <TextOutput/>
            </Output>
        </Logger>
    </Analysis>
    <Global>
        <Constant value="0.0" symbol="b_cf" name="cell-free transmission rate"/>
        <!--    <Disabled>
        <Constant value="0.0" symbol="b_c2c" name="cell2cell transmission rate"/>
    </Disabled>
-->
        <Constant value="0.0" symbol="D_V"/>
        <!--    <Disabled>
        <Constant value="0.0" symbol="l_c2c"/>
    </Disabled>
-->
        <Field value="0.0" symbol="Ve" name="extracellular virus">
            <Diffusion rate="0" well-mixed="true"/>
        </Field>
        <System time-scaling="1.0" name="extracellular viral load" time-step="1.0" solver="Runge-Kutta [fixed, O(4)]">
            <DiffEqn symbol-ref="Ve">
                <Expression>(p_V*V-c_V*Ve)*run_model</Expression>
            </DiffEqn>
        </System>
        <Constant value="0.0" symbol="V" name="virus production"/>
        <Event>
            <Condition>time >= 10</Condition>
            <Rule symbol-ref="run_model">
                <Expression>1</Expression>
            </Rule>
        </Event>
        <Variable value="0" symbol="run_model"/>
        <Constant value="0.0" symbol="c_V" name="clearance rate of extracellular virus"/>
        <Constant value="0.0" symbol="p_V" name="virus export rate"/>
    </Global>
    <CellTypes>
        <CellType name="target" class="biological">
            <Property value="0.0" symbol="V" name="viral load"/>
            <VolumeConstraint target="2" strength="1" name="target volume"/>
            <ConnectivityConstraint/>
            <SurfaceConstraint target="1" strength="3" mode="aspherity"/>
            <ChangeCellType newCellType="infected" name="cell becomes infected" time-step="1">
                <Condition>rand_uni(0,1) &lt;= p_cf and run_model==1</Condition>
                <Triggers>
                    <Rule symbol-ref="V">
                        <Expression>1</Expression>
                    </Rule>
                </Triggers>
            </ChangeCellType>
            <Function symbol="p_cf" name="probability to be infected at certain time point">
                <Expression>1-exp(-b_cf*Vi)</Expression>
            </Function>
            <Property value="0.0" symbol="Vi" name="viral load per cell"/>
            <Mapper name="extracellular to cellular viral load" time-step="1.0">
                <Input value="Ve"/>
                <Output symbol-ref="Vi" mapping="sum"/>
            </Mapper>
            <FreezeMotion>
                <Condition>run_model == 1</Condition>
            </FreezeMotion>
            <!--    <Disabled>
        <Function symbol="p_c2c" name="probability to be infected at certain time point">
            <Expression>1-exp(-b_c2c*V_c2c)</Expression>
        </Function>
    </Disabled>
-->
            <!--    <Disabled>
        <ChangeCellType newCellType="infected" name="cell becomes infected">
            <Condition>rand_uni(0,1) &lt;= p_c2c and run_model==1</Condition>
            <Triggers>
                <Rule symbol-ref="V">
                    <Expression>1</Expression>
                </Rule>
            </Triggers>
        </ChangeCellType>
    </Disabled>
-->
            <!--    <Disabled>
        <NeighborhoodReporter name="viral load of neighboring cells">
            <Input value="l_c2c*V" noflux-cell-medium="true" scaling="length"/>
            <Output symbol-ref="V_c2c" mapping="sum"/>
        </NeighborhoodReporter>
    </Disabled>
-->
            <!--    <Disabled>
        <Property value="0.0" symbol="V_c2c"/>
    </Disabled>
-->
        </CellType>
        <CellType name="infected" class="biological">
            <Property value="1" symbol="V" name="viral load"/>
            <VolumeConstraint target="2" strength="1" name="target volume"/>
            <ConnectivityConstraint/>
            <SurfaceConstraint target="1" strength="3" mode="aspherity"/>
            <FreezeMotion>
                <Condition>run_model == 1</Condition>
            </FreezeMotion>
            <!--    <Disabled>
        <NeighborhoodReporter name="viral load of neighboring cells">
            <Input value="l_c2c*V" noflux-cell-medium="true" scaling="length"/>
            <Output symbol-ref="V_c2c" mapping="sum"/>
        </NeighborhoodReporter>
    </Disabled>
-->
            <!--    <Disabled>
        <Property value="0.0" symbol="V_c2c"/>
    </Disabled>
-->
            <Mapper name="extracellular to cellular viral load" time-step="1.0">
                <Input value="Ve"/>
                <Output symbol-ref="Vi" mapping="sum"/>
            </Mapper>
            <Property value="0.0" symbol="Vi" name="viral load per cell"/>
        </CellType>
    </CellTypes>
    <CPM>
        <Interaction/>
        <ShapeSurface scaling="norm">
            <Neighborhood>
                <Order>6</Order>
            </Neighborhood>
        </ShapeSurface>
        <MonteCarloSampler stepper="edgelist">
            <MCSDuration value="1"/>
            <MetropolisKinetics temperature="1"/>
            <Neighborhood>
                <Order>2</Order>
            </Neighborhood>
        </MonteCarloSampler>
    </CPM>
    <CellPopulations>
        <Population type="target" size="99" name="target strat population">
            <InitDistribute number-of-cells="999" mode="random" probability="0.1&#xa;"/>
        </Population>
        <Population type="infected" size="1" name="infected ">
            <InitDistribute number-of-cells="1" mode="random" probability="0.1"/>
        </Population>
    </CellPopulations>
</MorpheusModel>
